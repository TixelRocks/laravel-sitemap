{
    "name": "tixelrocks/laravel-sitemap",
    "description": "Sitemap package for Laravel (Laravelium fork).",
    "keywords": ["laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^9.0",
        "illuminate/filesystem": "^9.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench-core": "^6.0",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Laravelium\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
